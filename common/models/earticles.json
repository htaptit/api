{
  "name": "earticles",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "hidden": [
    "sourcesId",
    "id"
  ],
  "properties": {
    "author": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "string",
      "length": 1024
    },
    "url": {
      "type": "string",
      "index": {
        "unique": true
      }
    },
    "urlToImage": {
      "type": "string",
      "length": 1024
    },
    "publishedAt": {
      "type": "date"
    },
    "content": {
      "type": "string",
      "length": 2048
    }
  },
  "validations": [],
  "scope": {
    "order": "publishedAt DESC",
    "limit": 20,
    "include": {"sources": "types"}
  },
  "relations": {
    "sources": {
      "type": "belongsTo",
      "model": "sources",
      "foreignKey": "sourcesId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
